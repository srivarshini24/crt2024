#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float a;
    scanf("%f",&a);
    printf("%d\n",(int)a);
    float b=ceil(a);
    float c=floor(a);
    printf("%.1f\n%.1f",b,c);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
