#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x,a,b,profit;
    scanf("%d%d%d",&x,&a,&b);
    profit=((x*a)-(x*b)-100);
    printf("%d",profit);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
