#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    float r1=a*b*c/100;
    printf("%.2f\n",r1);
    float r2=a+r1;
     printf("%.2f\n",r2);
    float r3=r1*2/100;
    printf("%.2f\n",r3);
    float r4=r2-r3;
    printf("%.2f\n",r4);
            
    
    
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
