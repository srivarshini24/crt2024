#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
     int countDigits(int num){
         int count=0;
         while(num!=0){
             num/=10;
             count++;        
         }
     return count;
 }
int main(){
    int n;
    scanf("%d",&n);
    int numDigits=countDigits(n);
    int remainder=n % numDigits;
    printf("%d\n",remainder);
    return 0;
}
     

    
