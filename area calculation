#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int s,l,b;
        float  r,a;
    scanf("%d%d%d%f\n",&s,&l,&b,&r);
    printf("%d\n",s*s);
    printf("%d\n",l*b);
    a = 3.14*r*r;
    printf("%.2f\n",a);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
